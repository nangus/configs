[alias]
	co = checkout
	ci = commit
	st = status
	dev = checkout develop
	mast = checkout master
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	cia = commit -a
[push]
	default = simple
[core]
	pager = /usr/bin/less
[user]
	name = Nicholas Orion Jones
	email = nojones@jpl.nasa.gov
