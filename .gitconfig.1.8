[alias]
#use this to add remote masters only needs to be done once
	rau = remote add upstream
#pull code from remote upstream and merge if different
	pu = !"git fetch origin -v; git fetch upstream -v; git merge upstream/master"
    posh = "push --set-upstream origin"
	co = checkout
	ci = commit -S
	st = status
	srp = checkout srp
	dev = checkout develop
	mast = checkout master
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	cia = commit -a -S
[push]
	default = simple
