#!/usr/bin/perl
print $ARGV[0]."\n";

@codes=(200,301,302,303,304,400,401,402,403,404,410,499,500,501,502,503,504);

if ($ARGV[0]){
}else{
  print "nothing passsed we are going to need a do over.\n";
  exit -1;
}
open IN, $ARGV[0];
%c=();
foreach $c(@codes){
  $c{$c}=0;
}
while ($in=<IN>){
  chomp($in);
  $c{$in}++;
}
$s="";
$count=0;
foreach $k(@codes){
  $s.= "\t".$c{$k};
  $count+=$c{$k};
}

print $count.$s."\n";
